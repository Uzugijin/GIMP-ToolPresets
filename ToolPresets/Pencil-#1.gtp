# GIMP tool preset file

(icon-name "gimp-tool-pencil")
(icon-pixbuf "")
(name "71")
(tool-options "GimpPencilOptions"
    (tool "gimp-pencil-tool")
    (foreground (color-rgb 1 1 1))
    (brush "1. Pixel")
    (dynamics "Basic Dynamics")
    (gradient "Standard")
    (palette "colorbending")
    (brush-size 1)
    (brush-spacing 0.20000000000000001)
    (jitter-amount 7.79)
    (dynamics-enabled no)
    (fade-unit percent))
(use-fg-bg yes)
(use-opacity-paint-mode yes)
(use-brush yes)
(use-dynamics no)
(use-mypaint-brush no)
(use-gradient no)
(use-pattern no)
(use-palette no)
(use-font no)

# end of GIMP tool preset file
